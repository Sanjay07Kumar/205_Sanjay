import React from "react";
const alertMsg=()=>
{
    alert("The alert message");
    console.log("message to developer");
}
const App=()=>
{
    return(
        <div className="App">
            <h1>Let us see the output of Javascript</h1>
            <button onClick={alertMsg}>Click</button>
        </div>
    );
}
export default App;
